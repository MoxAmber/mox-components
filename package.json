{
  "name": "mox-components",
  "version": "1.0.0",
  "description": "simple accessible web components",
  "type": "module",
  "exports": {
    "./components/*": "./lib/components/*/index.js",
    "./test-utils/*": "./lib/test-utils/*"
  },
  "customElements": "custom-elements.json",
  "files": [
    "lib/",
    "custom-elements.json"
  ],
  "scripts": {
    "prepare": "run-p -r build:* && husky install",
    "start": "run-p -r 'build:* -- --watch' storybook",
    "storybook": "storybook dev -p 6006",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build:manifest": "cem analyze --config .config/custom-elements-manifest.config.mjs",
    "test": "run-p -r \"storybook -- --no-open\" \"test:* -- {@}\" --",
    "test:storybook": "wait-on http://localhost:6006/ && test-storybook --browsers chromium firefox webkit",
    "lint": "eslint --config .config/eslint.config.cjs \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoxAmber/mox-components.git"
  },
  "keywords": [
    "lit",
    "components",
    "storybook"
  ],
  "author": "MoxAmber <amber@realemail.net>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MoxAmber/mox-components/issues"
  },
  "homepage": "https://github.com/MoxAmber/mox-components#readme",
  "dependencies": {
    "lit": "^3.0.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.1",
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@custom-elements-manifest/analyzer": "^0.9.0",
    "@eslint/eslintrc": "^2.1.3",
    "@eslint/js": "^8.53.0",
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/jest": "^0.2.3",
    "@storybook/test-runner": "^0.14.1",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/web-components": "^7.5.3",
    "@storybook/web-components-vite": "^7.5.3",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-plugin-lit": "^1.10.1",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-wc": "^2.0.4",
    "husky": "^8.0.0",
    "lint-staged": "^15.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "typescript": "^5.2.2",
    "vite-plugin-cem": "^0.5.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wait-on": "^7.2.0"
  }
}
