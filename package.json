{
  "name": "mox-components",
  "version": "1.0.0",
  "description": "simple accessible web components",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"src/**/*.ts\"",
    "storybook": "npm run manifest:watch & storybook dev -p 6006",
    "build-storybook": "storybook build",
    "manifest:generate": "cem analyze --litelement --globs 'src/**/*.ts' --exclude='src/**/*.stories.ts'",
    "manifest:watch": "npm run manifest:generate -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MoxAmber/mox-components.git"
  },
